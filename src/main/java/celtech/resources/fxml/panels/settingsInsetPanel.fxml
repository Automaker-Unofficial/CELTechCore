<?xml version="1.0" encoding="UTF-8"?>

<?import celtech.coreUI.components.printerstatus.*?>
<?import celtech.coreUI.components.material.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.shape.*?>
<?import jfxtras.styles.jmetro8.*?>

<HBox id="settingsInsetRoot" fx:id="settingsInsetRoot" maxWidth="360.0" minWidth="-Infinity" prefWidth="360.0" stylesheets="@../../css/JMetroDarkTheme.css" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="celtech.coreUI.controllers.panels.SettingsInsetPanelController">
    <children>
        <BorderPane id="AnchorPane" maxHeight="-1.0" HBox.hgrow="ALWAYS">
            <top>
                <VBox alignment="TOP_CENTER" fillWidth="true" maxWidth="-1.0" prefWidth="-1.0" spacing="25.0" BorderPane.alignment="CENTER">
                    <children>
                        <VBox alignment="CENTER" fillWidth="true" spacing="10.0">
                            <children>
                                <VBox minHeight="0.0" spacing="5.0">
                                    <children>
                                        <HBox fx:id="customProfileBox" alignment="CENTER" spacing="15.0">
                                            <children>
                                                <StackPane>
                                                    <children>
                                                        <ComboBox fx:id="customProfileChooser" prefWidth="270.0" promptText="%sidePanel_settings.customProfileSelectionLabel" styleClass="cmbCleanCombo">
                                                        </ComboBox>
                                                        <Label id="pleaseCreateProfile" fx:id="createProfileLabel" text="%sidePanel_settings.pleaseCreateProfile" />
                                                    </children>
                                                </StackPane>
                                                <Button fx:id="editPrintProfileButton" mnemonicParsing="false" onAction="#editPrintProfile" scaleY="0.9" style="-fx-background-color: light_grey;" text="..." />
                                            </children>
                                        </HBox>
                              <HBox alignment="CENTER_LEFT" spacing="15.0">
                                 <VBox.margin>
                                    <Insets />
                                 </VBox.margin>
                                 <children>
                                    <HBox alignment="CENTER" spacing="5.0">
                                       <children>
                                          <Label text="%extrusion.support" />
                                          <RadioButton fx:id="supportButton" contentDisplay="GRAPHIC_ONLY" graphicTextGap="0.0" mnemonicParsing="false" />
                                       </children>
                                    </HBox>
                                    <HBox alignment="CENTER" spacing="5.0">
                                       <children>
                                                        <Label text="%settingsInsetPanel.raft" />
                                          <RadioButton fx:id="raftButton" contentDisplay="GRAPHIC_ONLY" graphicTextGap="0.0" mnemonicParsing="false" />
                                       </children>
                                    </HBox>
                                              <HBox alignment="CENTER_LEFT" spacing="5.0">
                                                  <children>
                                                        <Label text="%settingsInsetPanel.brim" />
                                                        <Slider fx:id="brimSlider" blockIncrement="1.0" majorTickUnit="2.0" max="8.0" maxWidth="260" minorTickCount="1" showTickLabels="true" snapToTicks="true">
                                             <opaqueInsets>
                                                <Insets />
                                             </opaqueInsets>
                                          </Slider>
                                                  </children>
                                              </HBox>
                                 </children>
                                 <padding>
                                    <Insets left="10.0" />
                                 </padding>
                              </HBox>
                                        <HBox alignment="CENTER_LEFT" spacing="5.0"> 
                                            <children>
                                                <Label text="%settingsInsetPanel.raftSupportBrim" />
                                                <ComboBox fx:id="supportComboBox" prefWidth="234.0" />
                                            </children>
                                 <padding>
                                    <Insets left="10.0" />
                                 </padding>
                                        </HBox>
                                    </children>
                                </VBox>
                        <VBox spacing="5.0">
                           <children>
                              <HBox spacing="5.0">
                                 <children>
                                              <Label text="%sidePanel_settings.fillDensityLabel" />
                                    <HBox>
                                       <children>
                                          <Label fx:id="fillDensityPercent" text="%sidePanel_settings.fillDensityLabel" />
                                          <Label fx:id="fillDensityPercent1" layoutX="10.0" layoutY="10.0" text="\%" />
                                       </children>
                                    </HBox>
                                 </children>
                              </HBox>
                                        <HBox alignment="TOP_CENTER" spacing="15.0">
                                            <children>
                                                <Slider fx:id="fillDensitySlider" majorTickUnit="10.0" max="100.0" minorTickCount="5" showTickLabels="true" snapToTicks="true" HBox.hgrow="ALWAYS">
                                                    <HBox.margin>
                                                        <Insets left="10.0" right="10.0" />
                                                    </HBox.margin>
                                                </Slider>
                                            </children>
                                        </HBox>
                           </children>
                        </VBox>
                            </children>
                        </VBox>
                    </children>
                </VBox>
            </top>
            <HBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </HBox.margin>
        </BorderPane>
    </children>
</HBox>
