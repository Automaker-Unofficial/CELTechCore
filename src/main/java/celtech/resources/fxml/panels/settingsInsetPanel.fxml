<?xml version="1.0" encoding="UTF-8"?>

<?import celtech.coreUI.components.printerstatus.*?>
<?import celtech.coreUI.components.material.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.shape.*?>
<?import jfxtras.styles.jmetro8.*?>

<HBox id="settingsInsetRoot" fx:id="settingsInsetRoot" maxWidth="360.0" minWidth="-Infinity" prefWidth="360.0" stylesheets="@../../css/JMetroDarkTheme.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="celtech.coreUI.controllers.panels.SettingsInsetPanelController">
    <children>
        <BorderPane id="AnchorPane" maxHeight="-1.0" HBox.hgrow="ALWAYS">
            <top>
                <VBox alignment="TOP_CENTER" fillWidth="true" maxWidth="-1.0" prefWidth="-1.0" spacing="25.0" BorderPane.alignment="CENTER">
                    <children>
                        <VBox alignment="CENTER" fillWidth="true" spacing="5.0">
                            <children>
                                <VBox fx:id="customProfileVBox" minHeight="0.0" spacing="5.0">
                                    <children>
                                        <Label fx:id="customSettingsLabel" minHeight="0.0" styleClass="sidePanelDivider" text="%sidePanel_settings.customButtonLabel">
                                        </Label>
                                        <HBox alignment="CENTER" spacing="15.0">
                                            <children>
                                                <StackPane>
                                                    <children>
                                                        <ComboBox fx:id="customProfileChooser" prefWidth="270.0" promptText="%sidePanel_settings.customProfileSelectionLabel" styleClass="cmbCleanCombo">
                                                        </ComboBox>
                                                        <Label id="pleaseCreateProfile" fx:id="createProfileLabel" text="%sidePanel_settings.pleaseCreateProfile" />
                                                    </children>
                                                </StackPane>
                                                <Button fx:id="editPrintProfileButton" mnemonicParsing="false" onAction="#editPrintProfile" scaleY="0.9" style="-fx-background-color: light_grey;" text="..." />
                                            </children>
                                        </HBox>
                                    </children>
                                </VBox>
                                <VBox fx:id="nonCustomProfileVBox" fillWidth="true" spacing="5.0">
                                    <children>
                                        <Label styleClass="sidePanelDivider" text="%sidePanel_settings.supportMaterialLabel" />
                                        <HBox alignment="CENTER" spacing="5.0"> 
                                            <children>
                                                <Slider fx:id="supportSlider" majorTickUnit="1.0" max="1.0" maxWidth="260" minorTickCount="0" showTickLabels="true" showTickMarks="true" snapToTicks="true" />
                                            </children>
                                        </HBox>
                                        <Label styleClass="sidePanelDivider" text="%sidePanel_settings.raftLabel" />
                                        <HBox alignment="CENTER" spacing="5.0"> 
                                            <children>
                                                <Slider fx:id="raftSlider" majorTickUnit="1.0" max="1.0" maxWidth="260" minorTickCount="0" showTickLabels="true" showTickMarks="true" snapToTicks="true" />
                                            </children>
                                        </HBox>
                                        <Label styleClass="sidePanelDivider" text="%sidePanel_settings.brimLabel" />
                                        <HBox alignment="TOP_CENTER" spacing="15.0">
                                            <children>
                                                <Slider fx:id="brimSlider" majorTickUnit="1.0" max="3.0" maxWidth="260" minorTickCount="0" showTickLabels="true" showTickMarks="true" snapToTicks="true" />
                                            </children>
                                        </HBox>
                                        <Label styleClass="sidePanelDivider" text="%sidePanel_settings.fillDensityLabel" />
                                        <HBox alignment="TOP_CENTER" spacing="15.0">
                                            <children>
                                                <Slider fx:id="fillDensitySlider" majorTickUnit="10.0" max="100.0" minorTickCount="5" showTickLabels="true" showTickMarks="true" snapToTicks="true" HBox.hgrow="ALWAYS">
                                                    <HBox.margin>
                                                        <Insets left="10.0" right="10.0" />
                                                    </HBox.margin>
                                                </Slider>
                                            </children>
                                        </HBox>
                                    </children>
                                </VBox>
                            </children>
                        </VBox>
                    </children>
                </VBox>
            </top>
            <HBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </HBox.margin>
        </BorderPane>
    </children>
</HBox>
