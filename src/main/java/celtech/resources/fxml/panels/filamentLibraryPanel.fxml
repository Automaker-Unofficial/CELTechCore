<?xml version="1.0" encoding="UTF-8"?>

<?import celtech.coreUI.components.*?>
<?import celtech.coreUI.components.buttons.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import org.controlsfx.control.textfield.*?>

<GridPane id="materialsGridPane" vgap="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints halignment="RIGHT" hgrow="NEVER" percentWidth="45.0" />
        <ColumnConstraints halignment="LEFT" hgrow="NEVER" percentWidth="55.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
    </rowConstraints>
    <children>
        <Label text="%sidePanel_settings.MaterialName" GridPane.columnIndex="0" GridPane.rowIndex="2" />
        <Label text="%sidePanel_settings.MaterialType" GridPane.columnIndex="0" GridPane.rowIndex="3" />
        <Label text="%genericFirstLetterCapitalised.Colour" GridPane.columnIndex="0" GridPane.rowIndex="4" />
        <Label text="%sidePanel_settings.filamentDiameterLabel" GridPane.columnIndex="0" GridPane.rowIndex="5" />
        <Label text="%sidePanel_settings.filamentMultiplierLabel" GridPane.columnIndex="0" GridPane.rowIndex="6" />
        <Label text="%sidePanel_settings.extrusionRateMultiplierLabel" GridPane.columnIndex="0" GridPane.rowIndex="7" />
        <Label text="%sidePanel_settings.firstLayerBedTemp" GridPane.columnIndex="0" GridPane.rowIndex="8" />
        <Label text="%sidePanel_settings.bedTemp" GridPane.columnIndex="0" GridPane.rowIndex="9" />
        <Label text="%sidePanel_settings.firstLayerNozzleTemp" textAlignment="RIGHT" wrapText="true" GridPane.columnIndex="0" GridPane.rowIndex="10" />
        <Label text="%sidePanel_settings.nozzleTemp" GridPane.columnIndex="0" GridPane.rowIndex="11" />
        <Label text="%sidePanel_settings.ambientTemp" GridPane.columnIndex="0" GridPane.rowIndex="12" />
                      
        <TextField fx:id="name" maxWidth="-Infinity" GridPane.columnIndex="1" GridPane.rowIndex="2" />
        <ComboBox fx:id="material" styleClass="materialCombos" GridPane.columnIndex="1" GridPane.rowIndex="3" />
        <ColorPicker fx:id="colour" styleClass="materialCombos" GridPane.columnIndex="1" GridPane.rowIndex="4" />
        <HBox alignment="CENTER_LEFT" spacing="5.0" GridPane.columnIndex="1" GridPane.rowIndex="5"> 
            <children>
                <RestrictedNumberField fx:id="filamentDiameter" allowedDecimalPlaces="2" maxLength="4" prefColumnCount="4" />
                <Label text="%misc.mm" />
            </children>
        </HBox>
        <RestrictedNumberField fx:id="filamentMultiplier" allowedDecimalPlaces="2" maxLength="4" maxWidth="-Infinity" prefColumnCount="4" prefWidth="60.0" GridPane.columnIndex="1" GridPane.hgrow="NEVER" GridPane.rowIndex="6" />
        <RestrictedNumberField fx:id="feedRateMultiplier" allowedDecimalPlaces="2" maxLength="4" maxWidth="-Infinity" prefColumnCount="4" prefWidth="60.0" GridPane.columnIndex="1" GridPane.hgrow="NEVER" GridPane.rowIndex="7" />
        <HBox alignment="CENTER_LEFT" spacing="5.0" GridPane.columnIndex="1" GridPane.rowIndex="8"> 
            <children>
                <RestrictedNumberField fx:id="firstLayerBedTemperature" maxLength="3" prefColumnCount="4" />
                <Label text="%misc.degreesC" />
            </children>
        </HBox>
        <HBox alignment="CENTER_LEFT" spacing="5.0" GridPane.columnIndex="1" GridPane.rowIndex="9"> 
            <children>
                <RestrictedNumberField fx:id="bedTemperature" maxLength="3" prefColumnCount="4" />
                <Label text="%misc.degreesC" />
            </children>
        </HBox>
        <HBox alignment="CENTER_LEFT" fillHeight="false" spacing="5.0" GridPane.columnIndex="1" GridPane.rowIndex="10"> 
            <children>
                <RestrictedNumberField fx:id="firstLayerNozzleTemperature" maxLength="3" prefColumnCount="4" />
                <Label text="%misc.degreesC" />
            </children>
        </HBox>
        <HBox alignment="CENTER_LEFT" spacing="5.0" GridPane.columnIndex="1" GridPane.rowIndex="11"> 
            <children>
                <RestrictedNumberField fx:id="nozzleTemperature" maxLength="3" prefColumnCount="4" />
                <Label text="%misc.degreesC" />
            </children>
        </HBox>
        <HBox alignment="CENTER_LEFT" spacing="5.0" GridPane.columnIndex="1" GridPane.rowIndex="12"> 
            <children>
                <RestrictedNumberField fx:id="ambientTemperature" maxLength="3" prefColumnCount="4" />
                <Label text="%misc.degreesC" />
            </children>
        </HBox>
        <ComboBox id="cmbLibraryFilament" fx:id="cmbFilament" prefWidth="150.0" GridPane.columnIndex="1" />
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
</GridPane>
