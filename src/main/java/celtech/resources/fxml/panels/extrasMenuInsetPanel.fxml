<?xml version="1.0" encoding="UTF-8"?>

<?import celtech.coreUI.components.*?>
<?import celtech.coreUI.components.buttons.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import org.controlsfx.control.textfield.*?>

<VBox styleClass="blue-inset-panel" stylesheets="@../../css/JMetroDarkTheme.css" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox VBox.vgrow="ALWAYS">
            <children>
                <HBox minHeight="0.0" style="-fx-background-color: light_grey;" BorderPane.alignment="CENTER" VBox.vgrow="ALWAYS">
                    <children>
                        <VerticalMenu fx:id="panelMenu" />
                        <Separator id="calibrationMenuSeparator" maxWidth="-Infinity" minWidth="-Infinity" orientation="VERTICAL" prefWidth="5.0">
                            <HBox.margin>
                                <Insets bottom="30.0" top="95.0" />
                            </HBox.margin>
                        </Separator>
                        <VBox fx:id="insetNodeContainer">
                            <children>
                            </children>
                            <padding>
                                <Insets top="95.0" />
                            </padding>
                        </VBox>
                    </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
                </HBox>
                <StackPane minHeight="90.0">
                    <children>
                        <HBox alignment="CENTER_LEFT" maxHeight="-Infinity" minHeight="-Infinity" pickOnBounds="false" prefHeight="90.0" style="-fx-background-color: black;" BorderPane.alignment="CENTER">
                            <children>
                                <GraphicButtonWithLabel fxmlFileName="backwardButton" labelText="%buttonText.back" onAction="#okPressed" prefWidth="80.0" />
                            </children>
                        </HBox>
                        <HBox fx:id="buttonBoxContainer" alignment="CENTER" pickOnBounds="false" prefHeight="90.0" BorderPane.alignment="CENTER">
                            <children>
                            </children>
                        </HBox>
                    </children>
                </StackPane>
            </children>
         <VBox.margin>
            <Insets left="30.0" />
         </VBox.margin>
         <padding>
            <Insets top="60.0" />
         </padding>
        </VBox>
    </children>
    <VBox.margin>
        <Insets top="60.0" />
    </VBox.margin>
</VBox>
