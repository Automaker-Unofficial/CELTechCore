<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import celtech.coreUI.components.*?>
<?import celtech.coreUI.components.buttons.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="container" style="-fx-background-color: black;" stylesheets="@../../css/JMetroDarkTheme.css" VBox.vgrow="ALWAYS" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="celtech.coreUI.controllers.panels.PreferencesTopInsetPanelController">
    <children>
      <TopMenuStrip />        
        <HBox styleClass="inset-panel" VBox.vgrow="ALWAYS">
            <children>
            <VBox maxWidth="-Infinity" minWidth="-Infinity" prefWidth="30.0" style="-fx-background-color: black;" />
            <HBox>
               <children>
                  <VerticalMenu fx:id="preferencesMenu" maxWidth="-Infinity" minHeight="200.0" minWidth="-Infinity" prefWidth="360.0" />
                  <Separator id="calibrationMenuSeparator" maxWidth="-Infinity" minWidth="-Infinity" orientation="VERTICAL" prefWidth="5.0">
                     <HBox.margin>
                        <Insets bottom="30.0" top="130.0" />
                     </HBox.margin>
                  </Separator>
                  <VBox id="preferencesGridPane" fx:id="preferencesListContainer" spacing="30.0">
                     <HBox.margin>
                        <Insets top="130.0" />
                     </HBox.margin>
                     <children>
                        <GridPane fx:id="preferencesGridPane">
                          <columnConstraints>
                            <ColumnConstraints hgrow="NEVER" minWidth="-Infinity" prefWidth="250.0" />
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="-Infinity" prefWidth="150.0" />
                          </columnConstraints>
                          <rowConstraints>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                          </rowConstraints>
                           <VBox.margin>
                              <Insets left="60.0" />
                           </VBox.margin>
                        </GridPane>
                     </children></VBox>
               </children>
            </HBox>
            </children>
        </HBox>
        <BorderPane maxHeight="-Infinity" minHeight="-Infinity" prefHeight="90.0" styleClass="menubar">
            <center>
            </center>
         <center>
            <HBox alignment="CENTER" BorderPane.alignment="CENTER">
               <children>
                      <GraphicButton fx:id="backwardButton" fxmlFileName="backwardButton" onAction="#backwardPressed" />
               </children>
               <BorderPane.margin>
                  <Insets right="10.0" />
               </BorderPane.margin>
            </HBox>
         </center>
        </BorderPane>
    </children>
</VBox>
