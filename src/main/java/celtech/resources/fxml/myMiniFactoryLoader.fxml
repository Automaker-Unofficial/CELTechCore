<?xml version="1.0" encoding="UTF-8"?>

<?import celtech.coreUI.components.*?>
<?import celtech.coreUI.components.buttons.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="container" stylesheets="@../../css/JMetroDarkTheme.css" VBox.vgrow="ALWAYS" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="celtech.coreUI.controllers.MyMiniFactoryLoaderController">
    <children>        
        <HBox styleClass="inset-panel" VBox.vgrow="ALWAYS">
            <children>
                <VBox fx:id="webContentContainer" HBox.hgrow="ALWAYS" />
            </children>
        </HBox>
        <BorderPane styleClass="menubar">
            <right>
                <AddToProjectButton fx:id="addToProjectButton" onAction="#addToProjectPressed" />
            </right>
            <left>
                <HBox spacing="5" >
                    <children>
                        <CancelButton fx:id="cancelButton" onAction="#cancelPressed" />
                        <BackwardButton fx:id="backwardButton" onAction="#backwardPressed" />
                    </children>
                </HBox>
            </left>
            <center>
            </center>
        </BorderPane>
    </children>
</VBox>
