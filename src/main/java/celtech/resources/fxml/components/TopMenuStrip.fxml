<?xml version="1.0" encoding="UTF-8"?>

<?import celtech.coreUI.components.buttons.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.layout.*?>

<fx:root alignment="TOP_RIGHT" prefHeight="60.0"  pickOnBounds="false" fx:id="container" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <GraphicButton fx:id="materialsButton" fxmlFileName="helpButton" onAction="#materialsPressed" />
        <GraphicButton fx:id="aboutButton" fxmlFileName="helpButton" onAction="#aboutPressed" />
        <GraphicButton fx:id="preferencesButton" fxmlFileName="preferencesButton" onAction="#preferencesPressed" />
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
</fx:root>
