                <Label text="%sidePanel_settings.firstLayerBedTemp" GridPane.columnIndex="0" GridPane.rowIndex="6" />
                <Label text="%sidePanel_settings.bedTemp" GridPane.columnIndex="0" GridPane.rowIndex="7" />
                <Label text="%sidePanel_settings.firstLayerNozzleTemp" textAlignment="RIGHT" wrapText="true" GridPane.columnIndex="0" GridPane.rowIndex="8" />
