<VBox fx:id="container" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="370" minHeight="-Infinity" minWidth="-Infinity" styleClass="slideOutPanel" stylesheets="@../../css/JMetroDarkTheme.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="celtech.coreUI.controllers.utilityPanels.MaterialDetailsController">
