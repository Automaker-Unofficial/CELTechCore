                        <Label text="%misc.degreesC" />
                    </children>
                </HBox>
                <HBox spacing="5.0" GridPane.columnIndex="1" GridPane.rowIndex="10"> 
                    <children>
                        <RestrictedNumberField fx:id="ambientTemperature" maxLength="3" prefColumnCount="4" />
                        <Label text="%misc.degreesC" />
                    </children>
                </HBox>
            </children>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
