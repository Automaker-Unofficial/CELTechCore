                </HBox>
                <RestrictedNumberField fx:id="filamentMultiplier" maxLength="4" allowedDecimalPlaces="2" maxWidth="-Infinity" prefColumnCount="4" prefWidth="60.0" GridPane.columnIndex="1" GridPane.hgrow="NEVER" GridPane.rowIndex="4" />
