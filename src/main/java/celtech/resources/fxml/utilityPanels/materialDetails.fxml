                <Label text="%sidePanel_settings.ambientTemp" GridPane.columnIndex="0" GridPane.rowIndex="10" />
                
                <CustomTextField fx:id="name" maxWidth="-Infinity" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="0" />
                <ComboBox fx:id="material" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <ColorPicker fx:id="colour" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                <HBox spacing="5.0" GridPane.columnIndex="1" GridPane.rowIndex="3"> 
