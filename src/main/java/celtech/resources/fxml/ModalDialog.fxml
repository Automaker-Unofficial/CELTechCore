<?scenebuilder-stylesheet ../css/AutoMaker.css?>

<AnchorPane minHeight="-1.0" minWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" styleClass="modalDialogBackground" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="celtech.coreUI.controllers.ModalDialogController">
  <children>
    <VBox alignment="TOP_CENTER" layoutX="0.0" layoutY="0.0" prefHeight="-1.0" prefWidth="-1.0" spacing="0.0">
      <children>
        <Label fx:id="dialogTitle" alignment="CENTER" prefWidth="-1.0" styleClass="titleText" text="Title" />
        <Label fx:id="dialogMessage" alignment="CENTER" prefHeight="-1.0" prefWidth="-1.0" styleClass="labelText" text="This is a very long message that shows wrapping of lots of text ha ha ha blah blah blah" textAlignment="CENTER" wrapText="true" />
        <HBox fx:id="buttonHolder" alignment="TOP_CENTER" prefHeight="-1.0" prefWidth="-1.0" spacing="5.0">
          <padding>
            <Insets />
          </padding>
          <VBox.margin>
            <Insets top="8.0" />
          </VBox.margin>
        </HBox>
      </children>
      <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
    </VBox>
  </children>
  <padding>
    <Insets />
  </padding>
</AnchorPane>
